{
  "name": "NobiTik",
  "description": "Nobi",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["cookies"],
  "host_permissions": [
    "*://*.ecrm.vn/*",
    "*://*.nobi.pro/*",
    "*://*.seller-vn.tiktok.com/*",
    "*://*/*",
    "ws://*/*",
    "wss://*/*"
  ],
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
